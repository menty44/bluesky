<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.daniel.model.Store" table="stores">
        <id name="storeid" type="int" column="storeid">
            <generator class="increment" />
        </id>
        <property name="uuid">
            <column name="uuid" />
        </property>
        <property name="name">
            <column name="name" />
        </property>
        <property name="phoneno">
            <column name="phoneno" />
        </property>
        <property name="email">
            <column name="email" />
        </property>
        <property name="branch">
            <column name="branch" />
        </property>
        <property name="location">
            <column name="location" />
        </property>
        <property name="vatno">
            <column name="vatno" />
        </property>
        <property name="krapin">
            <column name="krapin" />
        </property>
    </class>
</hibernate-mapping>